#%Module1.0
module-whatis "beast"
module-whatis "Version: VERSION"
module-whatis "Description: beast"
module-whatis "Compiler: intel"
set beasthome /home/diag/opt/beast/v1.7.5
setenv BEAST $beasthome
setenv BEAST_HOME $beasthome
prepend-path PATH $beasthome/bin
if {  [ module-info mode load ]  } {
      module load beagle
} elseif {  [ module-info mode remove ]  } {
      module unload beagle
}
